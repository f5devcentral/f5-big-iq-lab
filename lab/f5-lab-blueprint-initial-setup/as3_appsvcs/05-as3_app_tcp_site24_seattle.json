{
  "class": "AS3",
  "action": "deploy",
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.18.0",
    "target": {
      "address": "10.1.1.7"
    },
    "backend": {
      "class": "Tenant",
      "site20tcp": {
        "class": "Application",
        "schemaOverlay": "AS3-F5-TCP-lb-built-in-profile-template-big-iq-default-v1",
        "template": "tcp",
        "serviceMain": {
          "class": "Service_TCP",
          "pool": "Pool",
          "enable": true,
          "profileTCP": "normal",
          "virtualPort": 80,
          "virtualAddresses": [
            "10.1.10.124"
          ],
          "profileAnalyticsTcp": {
            "use": "Analytics_TCP_Profile"
          }
        },
        "Pool": {
          "class": "Pool",
          "members": [
            {
              "adminState": "enable",
              "servicePort": 8083,
              "serverAddresses": [
                "10.1.20.114"
              ]
            },
            {
              "adminState": "enable",
              "servicePort": 8081,
              "serverAddresses": [
                "10.1.20.115"
              ]
            }
          ]
        },
        "Analytics_TCP_Profile": {
          "class": "Analytics_TCP_Profile",
          "collectCity": true,
          "collectRegion": true,
          "collectCountry": true,
          "collectPostCode": true,
          "collectContinent": true,
          "collectedByClientSide": true,
          "collectedByServerSide": true,
          "collectNexthop": true,
          "collectRemoteHostIp": true
        }
      }
    }
  }
}